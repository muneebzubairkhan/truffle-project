{
  "name": "cute-smart-contracts",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "truffle migrate --reset --network development",
    "____": "_____",
    "testRegular": "nodemon --exec \"truffle test\" --ext \".test.js .sol\"",
    "prettierRegular": "nodemon --watch contracts --exec \"npx prettier --write 'contracts/**/*.sol'\" --ext .sol",
    "compileRegular": "nodemon --watch contracts --exec \"truffle compile\" --ext .sol",
    "deployRegular": "sudo nodemon --watch contracts --exec \"truffle migrate --reset\" --ext .sol",
    "runLocalBlockchain": "ganache-cli",
    "_____": "_____",
    "startBscTestnet": "truffle migrate --network bscTestnet --all --reset",
    "startBscMainnet": "truffle migrate --network bscMainnet --reset && truffle run verify SomeSmartContract --network bscMainnet",
    "startRinkeby": "sudo truffle migrate --network rinkeby --reset",
    "startEthMainnet": "truffle migrate --network mainnet --reset && truffle run verify SomeSmartContract --network mainnet",
    "startInnovate": "truffle migrate --network %(grep DEPLOY_NETWORK .env)% --reset && truffle run verify SomeSmartContract --network $(grep DEPLOY_NETWORK .env)",
    "___": "_____",
    "beep1": "python -c \"import winsound; winsound.PlaySound('SystemExit', winsound.SND_ALIAS);\"",
    "beep2": "start wmplayer \"C:\\Windows\\Media\\Alarm10.wav\" && timeout 5 && taskkill /im wmplayer.exe"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@openzeppelin/contracts": "^4.4.1",
    "@truffle/hdwallet-provider": "^2.0.0",
    "dotenv": "^10.0.0",
    "erc721a": "3.3.0",
    "keccak256": "^1.0.6",
    "merkletreejs": "^0.2.31",
    "nodemon": "^2.0.13",
    "truffle-plugin-verify": "^0.5.20",
    "web3-utils": "^1.5.3"
  },
  "devDependencies": {
    "eth-gas-reporter": "^0.2.25",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19"
  }
}
